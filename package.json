{
  "name": "gitbook-plugin-json-schema",
  "version": "20.0.0",
  "description": "Gitbook plugin to convert JSON schema to markdown documentation",
  "main": "./dist/gitbook.compat.wrapper.js",
  "module": "./dist/module/index.js",
  "scripts": {
    "test": "jest",
    "build": "run-p build:*",
    "build:module": "babel src -d dist/module",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src -d dist --copy-files",
    "lint": "xo"
  },
  "engines": {
    "gitbook": ">=3.0.0"
  },
  "files": [
    "dist"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.intuit.com/Fuego/Fuego-Schema.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "deepmerge": "2.1.0",
    "html": "1.0.0",
    "json-schema-ref-parser": "5.0.3",
    "lodash": "4.17.5"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-preset-env": "^1.6.1",
    "cross-env": "5.1.4",
    "eslint-plugin-jest": "21.15.0",
    "eslint-plugin-prettier": "2.6.0",
    "gitbook-tester": "1.4.3",
    "jest": "22.4.3",
    "npm-run-all": "4.1.2",
    "xo": "0.20.3"
  },
  "babel": {
    "preset": [
      "env"
    ]
  },
  "xo": {
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier",
      "jest"
    ],
    "envs": [
      "jest"
    ],
    "parser": "babel-eslint",
    "rules": {
      "no-else-return": [
        "error",
        {
          "allowElseIf": true
        }
      ]
    }
  }
}
